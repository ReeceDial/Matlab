function[x]=MatrixMultiplication(A,B)
[xA,yA]=size(A);
[xB,yB]=size(B);
sum=0;
x=zeros(xA,yB);
if yA~=xB
    error('error: size mismatch');
elseif yA==xB
    x=size(xA,yB);
    k=1;
    for i=k:xA
        for j=1:yB
            sum=A(i,j)*B(i,j);
            x(i,j)=
            k=k+1;
        end
    end
end
end
